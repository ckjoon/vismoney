  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  target 'vismoneyTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'vismoneyUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
